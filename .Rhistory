Xbar25 = mean(x,trim = .25) # Compute 25% trimmed mean of x; call it Xbar25Xbar25
# Perform the bootstrap analysis, and call the results bootxbarbootxbar = bootstrap(x, nboot = 1000, mean, trim=.25)
summary(bootxbar$thetastar) #$ Summary statistics for the bootstrap samplespar(mfrow=c(1,2)) # Plot with one row, two columnshist(bootxbar$thetastar,col="cyan") #$ Make a histogram of the samplesqqnorm(bootxbar$thetastar) #$ Make a Q-Q plot of the samplesqqline(bootxbar$thetastar) #$ Add a line to the Q-Q plot
# Compute the standard deviation of the bootstrap-simulated statisticSEboot = sd(bootxbar$thetastar)#$ Use the t procedure to create the CICI = Xbar25 + c(-1,1) * tstar * SEboot # The c(-1,1)* code is for plus/minusCI # print the confidence interval
